{
  "nodeModulesDir": "auto",
  "tasks": {
    "dev": "deno run --watch --env-file --allow-env --allow-read --allow-write --allow-net main.ts",
    "test": "deno run --env-file --allow-env --allow-net test.ts",
    "all": "deno run --env-file --allow-env --allow-read --allow-write --allow-net bot.ts & deno run --env-file --allow-env --allow-read --allow-write --allow-net main.ts all",
    "all:no-log": "deno run --env-file --allow-env --allow-read --allow-write --allow-net bot.ts & deno run --env-file --allow-env --allow-read --allow-write --allow-net main.ts all --no-log",
    "art": "deno run --env-file --allow-env --allow-read --allow-write --allow-net main.ts art",
    "art:no-log": "deno run --env-file --allow-env --allow-read --allow-write --allow-net main.ts art --no-log",
    "label": "deno run --env-file --allow-env --allow-read --allow-write --allow-net main.ts label",
    "label:no-log": "deno run --env-file --allow-env --allow-read --allow-write --allow-net main.ts label --no-log",
    "frame": "deno run --env-file --allow-env --allow-read --allow-write --allow-net main.ts frame",
    "frame:no-log": "deno run --env-file --allow-env --allow-read --allow-write --allow-net main.ts frame --no-log",
    "joytick": "deno run --env-file --allow-env --allow-read --allow-write --allow-net main.ts joytick",
    "joytick:no-log": "deno run --env-file --allow-env --allow-read --allow-write --allow-net main.ts joytick --no-log",
    "bot": "deno run --env-file --allow-env --allow-read --allow-write --allow-net bot.ts",
    "build": "deno compile --allow-env --allow-read --allow-write --allow-net --output crawler main.ts",
    "build:windows": "deno compile --allow-env --allow-read --allow-write --allow-net --target x86_64-pc-windows-msvc --output crawler-windows.exe main.ts",
    "build:linux": "deno compile --allow-env --allow-read --allow-write --allow-net --target x86_64-unknown-linux-gnu --output crawler-linux main.ts",
    "build:macos": "deno compile --allow-env --allow-read --allow-write --allow-net --target x86_64-apple-darwin --output crawler-macos main.ts",
    "build:macos-arm": "deno compile --allow-env --allow-read --allow-write --allow-net --target aarch64-apple-darwin --output crawler-macos-arm64 main.ts",
    "build:all": "deno task build:windows && deno task build:linux && deno task build:macos && deno task build:macos-arm",
    "bundle": "deno bundle main.ts dist/crawler.js",
    "bundle:minify": "deno bundle main.ts dist/crawler.min.js && terser dist/crawler.min.js -o dist/crawler.min.js -c -m",
    "js:all": "node dist/crawler.js all",
    "js:all:no-log": "node dist/crawler.js all --no-log",
    "js:art": "node dist/crawler.js art",
    "js:art:no-log": "node dist/crawler.js art --no-log",
    "js:label": "node dist/crawler.js label",
    "js:label:no-log": "node dist/crawler.js label --no-log",
    "js:frame": "node dist/crawler.js frame",
    "js:frame:no-log": "node dist/crawler.js frame --no-log",
    "js:joytick": "node dist/crawler.js joytick",
    "js:joytick:no-log": "node dist/crawler.js joytick --no-log"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/fs": "jsr:@std/fs@1",
    "p-queue": "npm:p-queue@^8.0.1",
    "discord.js": "npm:discord.js@^14.19.3"
  }
}
